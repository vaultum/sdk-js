{
  "name": "@vaultum/sdk",
  "version": "0.1.1",
  "description": "TypeScript SDK for Vaultum smart account - ERC-4337 account abstraction wallet",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "generate": "node scripts/generate-from-openapi.js",
    "build": "pnpm generate && tsup src/index.ts --format esm,cjs --dts --clean",
    "prepack": "pnpm run build",
    "build:full": "pnpm generate && tsup src/index.ts --format esm,cjs --dts --clean",
    "dev": "tsup src/index.ts --format esm,cjs --dts --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm build"
  },
  "keywords": [
    "vaultum",
    "wallet",
    "account-abstraction",
    "erc4337",
    "sdk"
  ],
  "author": "Vaultum Team",
  "license": "MIT",
  "homepage": "https://github.com/vaultum/vaultum#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vaultum/vaultum.git",
    "directory": "vaultum-sdk-js"
  },
  "bugs": {
    "url": "https://github.com/vaultum/vaultum/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^1.6.1",
    "openapi-typescript": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@vaultum/abi": "0.2.0-alpha",
    "cross-fetch": "^4.0.0",
    "ethers": "^6.0.0",
    "openapi-fetch": "^0.8.0"
  },
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  }
}
